#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node/bin/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node/bin/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules/@babel/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/@babel+node@7.24.8_@babel+core@7.24.9/node_modules:/home/mint_01/Desktop/ALX/alx-backend-javascript/0x00-ES6_basic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel-node.js" "$@"
else
  exec node  "$basedir/../../bin/babel-node.js" "$@"
fi
